@page
@model EmpresaListarFuncionarios.Pages.funcionario.IndexModel

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Funcionario[0].IdEmpresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funcionario[0].IdCargo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funcionario[0].NomeFuncionario)
